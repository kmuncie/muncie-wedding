{
  "locals": {
    "url": "http://muncie.us",
    "owner": "Kevin Muncie",
    "name": "muncie",
    "description": "Website for our wedding!",
    "dateDescriptionFormat": "dddd, MMMM Do YYYY",
    "root": "/",
    "assets": "/assets",
    "robotsDisallowed": ""
  },
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "plugins": [
    "wintersmith-nunjucks",
    "wintersmith-livereload",
    "./plugins/paginator.coffee"
  ],
  "paginator": {
    "perPage": 3
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  }
}